https://www.geeksforgeeks.org/problems/kadanes-algorithm-1587115620/1
class Solution {
  public:
    int maxSubarraySum(vector<int> &arr) {
        // Code here
    
       int sumfirst=0;
        int sumend=INT_MIN;
        for(int i=0;i<arr.size();i++)
        {
            sumfirst=sumfirst+arr[i];
             if(sumend<sumfirst)
                sumend=sumfirst;
             if(sumfirst<0)
                sumfirst=0;
        }
        return sumend;
    }
};
